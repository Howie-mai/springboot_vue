<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhku.mh.mapper.UserPwdMapper">
    <resultMap id="BaseResultMap" type="com.zhku.mh.entities.UserPwd">
        <id column="id" property="id" jdbcType="INTEGER" ></id>
        <result column="pwd" property="pwd" jdbcType="VARCHAR"></result>
    </resultMap>

    <insert id="insertUserPwd">
        INSERT INTO t_user_pwd (pwd) VALUES (#{pwd});
    </insert>

</mapper>